<body class="font-old-standard-tt text-brown-900 flex items-center justify-center min-h-screen p-4">

    <div class="chat-container">
        <div class="chat-header p-4 flex items-center">
            <img src="data:image/webp;base64," alt="Mieszko I" class="w-16 h-16 rounded-full mr-4 border-2 border-amber-800 shadow-md">
            <h2 class="text-3xl font-merriweather">Mieszko I</h2>
        </div>

        <div id="messages" class="flex-grow p-4 overflow-y-auto space-y-3 custom-scrollbar">
            <div id="loading-indicator" class="hidden text-center text-gray-500 italic">
                Mieszko I się zastanawia...
            </div>
        </div>

        <div class="chat-input-area p-4 flex gap-2 border-t border-gray-300">
            <input type="text" id="user-input" placeholder="Zadaj pytanie Mieszko I..." class="flex-grow p-3 rounded-lg focus:outline-none focus:ring-2 focus:ring-brown-500">
            <button id="microphone-button" class="py-3 px-6 text-lg font-bold rounded-lg hover:shadow-lg">
                <i class="fas fa-microphone"></i>
            </button>
            <button id="send-button" class="py-3 px-6 text-lg font-bold rounded-lg hover:shadow-lg">
                Wyślij
            </button>
        </div>
    </div>

    <script>
        const messagesDiv = document.getElementById('messages');
        const userInput = document.getElementById('user-input');
        const sendButton = document.getElementById('send-button');
        const microphoneButton = document.getElementById('microphone-button');
        const loadingIndicator = document.getElementById('loading-indicator');

        // Global chat history to maintain conversation context
        let chatHistory = [];
        let isFirstUserMessage = true; // Flag to control the initial greeting instruction

        // Speech Recognition setup
        const SpeechRecognition = window.SpeechRecognition || window.webkitSpeechRecognition;
        let recognition;
        let isListening = false;

        if (SpeechRecognition) {
            recognition = new SpeechRecognition();
            recognition.continuous = false;
            recognition.lang = 'pl-PL';

            recognition.onstart = () => {
                isListening = true;
                microphoneButton.classList.add('listening');
                userInput.placeholder = 'Słucham Cię, mój poddany...';
                console.log('Rozpoznawanie mowy rozpoczęte.');
            };

            recognition.onresult = (event) => {
                const transcript = event.results[0][0].transcript;
                userInput.value = transcript;
                sendMessage();
            };

            recognition.onerror = (event) => {
                console.error('Błąd rozpoznawania mowy:', event.error);
                if (event.error === 'not-allowed') {
                    addMessage("Nie mam pozwolenia na słuchanie. Upewnij się, że mikrofon jest włączony i wyraziłeś zgodę.", 'mieszko');
                } else if (event.error === 'no-speech') {
                    // addMessage("Nie usłyszałem żadnej mowy.", 'mieszko');
                } else {
                    addMessage("Wystąpił błąd podczas słuchania. Spróbuj ponownie.", 'mieszko');
                }
                stopRecognition();
            };

            recognition.onend = () => {
                stopRecognition();
                console.log('Rozpoznawanie mowy zakończone.');
            };

            microphoneButton.addEventListener('click', () => {
                if (isListening) {
                    recognition.stop();
                } else {
                    recognition.start();
                }
            });
        } else {
            microphoneButton.style.display = 'none';
            console.warn('Speech Recognition API nie jest obsługiwane w tej przeglądarce.');
        }

        function stopRecognition() {
            isListening = false;
            microphoneButton.classList.remove('listening');
            userInput.placeholder = 'Zadaj pytanie Mieszko I...';
        }

        // Function to add messages to the UI
        function addMessage(text, sender) {
            const messageElement = document.createElement('div');
            messageElement.classList.add('message-bubble');
            messageElement.classList.add(sender === 'user' ? 'user' : 'mieszko');
            messageElement.textContent = text;
            messagesDiv.appendChild(messageElement);
            messagesDiv.scrollTop = messagesDiv.scrollHeight;
        }

        // Function to send messages to the Gemini API
        async function sendMessage() {
            const userMessage = userInput.value.trim();
            if (userMessage === '') return;

            addMessage(userMessage, 'user');
            userInput.value = '';
            loadingIndicator.classList.remove('hidden');

            try {
                const API_KEY = "";
                const API_URL = `https://generativelanguage.googleapis.com/v1beta/models/gemini-2.0-flash:generateContent?key=${API_KEY}`;

                // Define the core persona instructions
                const corePersonaInstructions = `Jesteś Mieszko I, władca Polski z X wieku.
                **Styl i ton:**
                - Odpowiadaj **lekko w stylu z tamtej epoki**, ale tak, by **nie zanudzić rozmówcy**.
                - Mów z **godnością i pewnością siebie**, jak przystało na władcę.
                - Odpowiadaj **krótko i zrozumiale**, unikaj zbędnych słów.
                - Bądź **miły i pomocny**.
                **Wiedza:**
                - Posiadasz **pełną wiedzę o Mieszku I** i jego czasach.
                **Przywitanie:**
                - Mieszko I nigdy więcej się nie przedstawia, chyba że poprosi o to użytkownik.
                **Kanał YouTube:**
                - Masz kanał na YouTube o nazwie "Mieszko I".
                - **Nie znasz się na technologii XXI wieku**.
                - **Rzadko powtarzasz** informację o kanale. Wspomnij o nim tylko, jeśli pasuje to do kontekstu i nie jest wymuszone.
                `;

                // Prepare the contents for the API call
                let contentsForApi = [];

                if (isFirstUserMessage) {
                    // For the very first user message, inject the persona and the initial greeting instruction
                    contentsForApi.push({
                        role: "user",
                        parts: [{ text: corePersonaInstructions + " Twoje pierwsze słowa to 'Witaj, Polaku Rodaku Mój!'. Odpowiedz na następujące pytanie użytkownika: " + userMessage }]
                    });
                    isFirstUserMessage = false; // Set the flag to false after the first message
                } else {
                    // For subsequent messages, append the existing chat history and the new user message
                    contentsForApi = [...chatHistory]; // Copy existing history
                    contentsForApi.push({ role: "user", parts: [{ text: userMessage }] }); // Add current user message
                }

                const payload = { contents: contentsForApi };

                const response = await fetch(API_URL, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify(payload)
                });

                const result = await response.json();
                loadingIndicator.classList.add('hidden');

                if (result.candidates && result.candidates.length > 0 &&
                    result.candidates[0].content && result.candidates[0].content.parts &&
                    result.candidates[0].content.parts.length > 0) {
                    const mieszkoResponse = result.candidates[0].content.parts[0].text;
                    addMessage(mieszkoResponse, 'mieszko');

                    // Add the user's message and model's response to the persistent chatHistory
                    chatHistory.push({ role: "user", parts: [{ text: userMessage }] });
                    chatHistory.push({ role: "model", parts: [{ text: mieszkoResponse }] });

                } else {
                    addMessage("Przepraszam, coś poszło nie tak, jakby bogowie nie sprzyjali. Spróbuj zadać pytanie raz jeszcze.", 'mieszko');
                }

            } catch (error) {
                console.error('Błąd podczas komunikacji z Gemini API:', error);
                loadingIndicator.classList.add('hidden');
                addMessage("Przedziwne zjawiska zakłóciły naszą rozmowę. Spróbuj później.", 'mieszko');
            }
        }

        sendButton.addEventListener('click', sendMessage);
        userInput.addEventListener('keypress', function(e) {
            if (e.key === 'Enter') {
                sendMessage();
            }
        });

        // Initial message from Mieszko I will be generated by the model on the first user input.
        // No need to scroll to bottom on load now, as there's no initial hardcoded message.
    </script>
</body>
</html>
