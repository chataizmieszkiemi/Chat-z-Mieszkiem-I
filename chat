    <title>Chat z Mieszkiem I</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <link href="https://fonts.googleapis.com/css2?family=Merriweather:wght@400;700&family=Old+Standard+TT:ital,wght@0,400;0,700;1,400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <style>
        /* Stylizacja tła */
        body {
            /* Zastąp ten URL adresem URL Twojego obrazka z postarzanego papieru */
            background-image: url('data:image/jpeg;base64,');
            background-size: cover; /* Pokrywa całą powierzchnię */
            background-repeat: no-repeat;
            background-attachment: fixed; /* Tło nie przewija się wraz z treścią */
            background-position: center center;
            font-family: 'Old Standard TT', serif; /* Czcionka przypominająca stary styl */
            color: #222222; /* Bardzo ciemny szary/czarny kolor tekstu dla lepszej czytelności */
            display: flex;
            justify-content: center;
            align-items: center;
            min-height: 100vh;
            padding: 1rem; /* Dodatkowe dopełnienie */
        }

        /* Stylizacja głównego kontenera czatu */
        .chat-container {
            background-color: rgba(255, 255, 255, 0.85); /* Lekko przezroczyste białe tło dla czytelności */
            border: 3px solid #6F4E37; /* Ciemnobrązowa ramka */
            border-radius: 10px;
            box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2);
            max-width: 500px;
            width: 100%;
            overflow: hidden;
            display: flex;
            flex-direction: column;
            min-height: 500px; /* Minimalna wysokość czatu */
        }

        /* Stylizacja nagłówka czatu */
        .chat-header {
            background-color: #6F4E37; /* Ciemnobrązowy nagłówek */
            color: #F5DEB3; /* Jasny tekst nagłówka */
            border-bottom: 2px solid #4A2E00;
        }

        /* Stylizacja dymków wiadomości */
        .message-bubble {
            color: black; /* Czarny kolor czcionki dla wiadomości */
            border: 1px solid #D2B48C; /* Delikatna ramka */
            border-radius: 8px;
            padding: 0.75rem 1rem;
            margin-bottom: 0.5rem;
            max-width: 80%;
            word-wrap: break-word;
            box-shadow: 1px 1px 3px rgba(0,0,0,0.1);
        }
        .message-bubble.mieszko {
            background-color: #E6D8C0; /* Lekki brąz dla wiadomości Mieszka */
            align-self: flex-start;
        }
        .message-bubble.user {
            background-color: #F8F4E3; /* Jeszcze jaśniejszy, kremowy brąz dla wiadomości użytkownika */
            align-self: flex-end;
        }

        /* Stylizacja inputu i przycisku */
        .chat-input-area input {
            background-color: #FDF5E6;
            border: 1px solid #D2B48C;
            border-radius: 5px;
            color: #4A2E00;
        }
        .chat-input-area button {
            background-color: #6F4E37; /* Ciemnobrązowy przycisk (zastępuje niebieski) */
            color: #F5DEB3;
            border-radius: 5px;
            transition: background-color 0.2s ease;
        }
        .chat-input-area button:hover {
            background-color: #8B4513; /* Jaśniejszy brąz po najechaniu */
        }
        /* Styl dla czcionki Merriweather dla nagłówków */
        h1, h2, h3, h4, h5, h6 {
            font-family: 'Merriweather', serif;
        }
        /* Styl dla przycisku mikrofonu */
        #microphone-button {
            background-color: #D2B48C; /* Kolor brązu */
            color: #4A2E00; /* Ciemny tekst */
            border: 1px solid #6F4E37;
        }
        #microphone-button.listening {
            background-color: #FF6347; /* Czerwony, gdy nasłuchuje */
            color: white;
        }
